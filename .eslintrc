{
  "extends": "airbnb",
  "rules": {
    "arrow-body-style": 0,
    "no-use-before-define": 0,
    "no-shadow": 0,
    "no-confusing-arrow": 0,
    "no-param-reassign": 0,
    "no-else-return": 0,
    "quote-props": 0,
    "padded-blocks": 0,
    "prefer-arrow-callback": 0,
    "func-names": 0,
    "react/jsx-no-bind": 0,
    "react/jsx-uses-react": 1,
    "react/no-multi-comp": 0,
    "react/prefer-stateless-function": 0
  },
  "plugins": [
    "flowtype",
    "flow-vars"
  ],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6,
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true
    }
  }
}
